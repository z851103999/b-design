{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"prettier.vueIndentScriptAndStyle": false,
	"eslint.enable": true,
	"eslint.lintTask.enable": true,
	"eslint.quiet": false,
	"eslint.probe": [
		"javascript",
		"javascriptreact",
		"typescriptreact",
		"typescript",
		"vue",
		"vuejsx"
	],
